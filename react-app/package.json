{
  "name": "react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "postinstall": "cp -r node_modules/pyodide/ public/pyodide/",
    "predeploy": "npm run build",
    "deploy": "npm run build && copyfiles -f dist/index.html dist && copyfiles -S dist/index.html dist/404.html && gh-pages -d dist"
  },
  "dependencies": {
    "dagre": "^0.8.5",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "pyodide": "^0.28.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.8.0",
    "reactflow": "^11.11.4",
    "tinyqueue": "^3.0.0",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/dagre": "^0.7.53",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
}
